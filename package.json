{
  "name": "log-server",
  "version": "1.0.0",
  "description": "Displays logs that it receives from test devices",
  "main": "server/index.ts",
  "scripts": {
    "build": "rimraf build && webpack",
    "production": "cd ./build && node .",
    "dev": "nodemon server/index.ts",
    "dev:docker": "docker build -t log-server:latest .",
    "dev:dash": "node ./start_debug_dashboard.js",
    "start": "cd ./build && node .",
    "tmux": "tmux new -d 'yarn build && yarn start'"
  },
  "dependencies": {
    "@types/cookie": "^0.3.3",
    "@types/dotenv": "^6.1.1",
    "@types/lodash": "^4.14.123",
    "@types/node": "^11.12.1",
    "@types/pino": "^5.8.5",
    "@types/sanitize-html": "^1.18.3",
    "@types/shelljs": "^0.8.3",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "dotenv": "^6.2.0",
    "fastify": "^2.3.0",
    "fastify-cookie": "^3.0.2",
    "fastify-static": "^2.4.0",
    "foobar-ipsum": "^1.0.3",
    "knex": "^0.16.5",
    "lodash": "^4.17.11",
    "lorem-ipsum": "^2.0.2",
    "moment": "^2.24.0",
    "nodemon": "^1.18.10",
    "pino": "^5.11.1",
    "pino-pretty": "^2.6.0",
    "sanitize-html": "^1.20.1",
    "shelljs": "^0.8.3",
    "sqlite3": "^4.0.8",
    "ts-lint": "^4.5.1",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.2",
    "tslint": "^5.13.1",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
